@model IEnumerable<GAM.Models.Laboratorio.Espermograma>

@{
    ViewData["Title"] = "Lista de Espermogramas";
}

<h2>Lista de Espermogramas</h2>

<p>

</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DataEspermograma)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Volume)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cor)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Viscosidade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Liquefacao)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ph)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Observacoes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ConcentracaoEspermatozoides)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GrauA)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GrauB)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GrauC)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GrauD)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Leucocitos)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vitalidade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ObservacoesConcentracao)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Amostra)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataEspermograma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Volume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Viscosidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Liquefacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ph)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacoes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConcentracaoEspermatozoides)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrauA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrauB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrauC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrauD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Leucocitos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vitalidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObservacoesConcentracao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amostra.AmostraId)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.EspermogramaId"><button type="button" class="btn btn-default"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button></a>
                <a asp-action="Edit" asp-route-id="@item.EspermogramaId"><button type="button" class="btn btn-default"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button></a>
                <a asp-action="Delete" asp-route-id="@item.EspermogramaId"><button type="button" class="btn btn-default"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></button></a>

            </td>
        </tr>
}
    </tbody>
</table>
